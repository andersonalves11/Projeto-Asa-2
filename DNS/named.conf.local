options {
listen-on port 53 { any; };
directory "/var/cache/bind";
allow-query { any; };	
};

zone "ifrn.asa.br" {
	type master;
	file "/etc/bind/ifrn.asa.br";	
	allow-transfer { any; };
	allow-query { any; };
};

zone "1.168.192.in-addr.arpa" {
    type master;
    file "/etc/bind/db.192.168.1";
};
